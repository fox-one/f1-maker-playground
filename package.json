{
  "name": "f1ex-admin",
  "version": "1.0.2",
  "description": "F1EX Admin Panel",
  "private": true,
  "scripts": {
    "dev": "yarn start:dev",
    "start": "yarn start:dev",
    "start:dev": "cross-env APP_TYPE=dev umi dev",
    "start:f1ex": "cross-env APP_TYPE=f1ex umi dev",
    "build": " cross-env APP_TYPE=f1ex umi build",
    "build:local": "cross-env APP_TYPE=local umi build",
    "build:dev": "cross-env APP_TYPE=dev umi build",
    "build:demo": "yarn presite && cross-env APP_TYPE=demo umi build",
    "build:f1ex": "yarn presite && cross-env APP_TYPE=f1ex umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "lint": "eslint --ext .js src mock tests --fix&& yarn lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests --fix&& yarn lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js --fix",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --config ./.prettierrc --write ./src/**/**/*"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "antd": "^3.11.6",
    "bizcharts": "^3.4.2",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "blueimp-md5": "^2.10.0",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "enquire-js": "^0.2.1",
    "f1-passport": "^0.0.18",
    "fs-promise": "^2.0.3",
    "hash.js": "^1.1.5",
    "localforage": "^1.7.3",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^5.0.0",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.2",
    "rc-animate": "^2.4.4",
    "react": "^16.6.3",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.5.1",
    "react-fittext": "^1.0.0",
    "react-router-dom": "^4.3.1",
    "typescript": "^3.1.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "antd-pro-merge-less": "^1.0.0",
    "antd-theme-webpack-plugin": "^1.1.8",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.7.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "merge-umi-mock-data": "^0.0.3",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.15.2",
    "pro-download": "^1.0.1",
    "stylelint": "^9.8.8",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.3.1",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-react": "^1.2.0"
  },
  "optionalDependencies": {
    "puppeteer": "^1.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "**/*.{js,jsx}": [
      "yarn lint-staged:js",
      "git add"
    ],
    "**/*.less": "stylelint --syntax less --fix"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  }
}
