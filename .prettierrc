{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "bracketSpacing": true,
  "arrowParens" : "avoid",
  "jsxBracketSameLine" : true,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "babylon" }
    }
  ]
}
